    "dayjs": "^1.11.10",
    "iron-session": "^8.0.1",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.2",
    "react-number-format": "^5.3.4",
    "sweetalert2": "^11.10.7"
  },
