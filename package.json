{
  "name": "customer-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "apigen": "SET NODE_TLS_REJECT_UNAUTHORIZED=0 && npm run apigen:prepare && npm run apigen:process:api-customer-service && npm run apigen:process:api-kyc && npm run apigen:process:correction",
